<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
  <listeners>
  	<listener class-name="generateReport.Listeners"></listener>
  </listeners>
  <test thread-count="5" name="TestNG">
  <parameter name="browser" value="chrome"></parameter>
    <classes>
      <class name="testscript.LoginTest">
     <methods>
      <include name="verifyUserLoginWithValidCredentials"></include>
      <include name="verifyUserLoginWithCorrectUnameAndIncorrectPassword"></include>
     <!--  <exclude name="verifyUserLoginWithIncorrectUnameAndCorrectPassword"></exclude>
      <exclude name="verifyUserLoginWithIncorrectUnameAndIncorrectPassword"></exclude> -->
      </methods>
      </class>
    </classes>
    
  </test>
</suite>
